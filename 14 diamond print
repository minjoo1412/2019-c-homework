#include <iostream>
using namespace std;

void dia(int k);

int main() {
	int t, k;
	cin >> t;
	for (int i = 0; i < t; i++) {
		cin >> k;
		dia(k);
	}
	return 0;
}

void dia(int k) {
	for (int i = 1; i <= k/2+1; i++) {
		for (int j = k/2-i; j > 0; j--) {
			cout << "*";
		}
		for (int j = 0; j < 2*i-1; j++) {
			cout << "+";
		}
		for (int j = k / 2-i; j > 0; j--) {
			cout << "*";
		}
		cout << endl;
	}	
	for (int i = 0; i < k/2; i++) {
		for (int j = 0; j <= i; j++) {
			cout << "*";
		}
		for (int j = 0; j < 2*(k/2-i); j++) {
			cout << "+";
		}
		for (int j = 0; j <= i; j++) {
			cout << "*";
		}
		cout << endl;
	}
}
