#include <iostream>
#include <String>

using namespace std;

void plus1(int& a, int& b, int c) {
	a += c;
	b += c;
	return;
}

int main() {
	cout << "hello, world" << endl;
	cout << "껄껄껄, 살려주세요" << endl;

	cout << "insert num" << endl;
	int num;
	cin >> num;
	for (int i = 1; i <= num; i++) {
		for (int j = 1; j <= i; j++) {
			cout << (2*j -1) << " ";
		}cout << endl;
	}

	cout << "hello world" << endl;
	int num1 = 30;
	int num2 = 56;
	int num3 = 12;
	plus1(num1, num2, num3);
	cout << num1 << " hi " << num2 << " hi " << endl;

	

	return 0;
}
